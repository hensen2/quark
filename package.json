{
  "name": "vite-react-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "format": "biome format --write",
    "lint": "biome lint --write",
    "check": "biome check --verbose",
    "check:fix": "biome check --write --verbose",
    "test": "vitest",
    "test:unit": "vitest --project=unit",
    "test:integration": "vitest --project=browser",
    "test:ui": "vitest --ui",
    "coverage": "vitest --coverage",
    "tsc": "tsc -b --verbose"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.1",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-router": "^1.102.1",
    "@tanstack/react-table": "^8.20.6",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.473.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@tailwindcss/postcss": "^4.0.0",
    "@tailwindcss/vite": "^4.0.3",
    "@tanstack/react-query-devtools": "^5.66.0",
    "@tanstack/router-devtools": "^1.102.1",
    "@tanstack/router-plugin": "^1.102.2",
    "@types/node": "^22.13.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/browser": "^3.0.1",
    "@vitest/coverage-v8": "^3.0.1",
    "globals": "^15.14.0",
    "msw": "^2.7.0",
    "playwright": "^1.49.1",
    "postcss": "^8.5.1",
    "tailwindcss": "^4.0.0",
    "typescript": "~5.7.2",
    "vite": "^6.0.5",
    "vitest": "^3.0.1",
    "vitest-browser-react": "^0.0.4"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
